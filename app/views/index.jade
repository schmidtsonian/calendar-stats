include _master.jade

block body

    section.wrapper.disable
        ul.levels
            - for (var x = 10; x > 0; x--)
                li: .levels__number= x * 10
        
        ul.bars
            - for (var x = 0; x < 12; x++)
                li.js-bar 
                    .bar.bar__shadow
                    .bar.bar__top
        ul.months
            li jan
            li feb
            li mar
            li apr
            li may
            li jun
            li jul
            li aug
            li sept
            li oct
            li nov
            li dic

        .year 
            span(class='js-year')
            span(class='js-year')
            span(class='js-year')
            span(class='js-year')

